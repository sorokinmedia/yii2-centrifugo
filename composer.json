{
	"name": "sorokinmedia/yii2-centrifugo",
	"description": "Centrifugo component for Yii2 framework",
	"type": "yii2-extension",
	"keywords": ["yii2", "yii 2", "centrifugo", "client"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Ruslan Gilyazetdinov",
			"email": "ma3obblu@gmail.com",
			"homepage": "https://www.facebook.com/Ma3oBblu"
		}
	],
	"support": {
		"issues": "https://github.com/sorokinmedia/yii2-centrifugo/issues?state=open",
		"source": "https://github.com/sorokinmedia/yii2-centrifugo"
	},
	"autoload": {
		"psr-4": {
			"sorokinmedia\\centrifugo\\": "src/"
		}
	},
	"require": {
		"yiisoft/yii2": ">=2.0.30",
		"centrifugal/phpcent": "dev-master"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	}
}
